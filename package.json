{
  "name": "ant-design-vue-admin",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://wkl007.github.io/ant-design-vue-admin",
  "scripts": {
    "commit": "git-cz",
    "serve": "vue-cli-service serve",
    "build:dev": "vue-cli-service build --mode dev",
    "build:pro": "vue-cli-service build --mode production",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "deploy": "deploy-cli-service deploy",
    "deploy:dev": "deploy-cli-service deploy --mode dev",
    "deploy:pro": "deploy-cli-service deploy --mode pro",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "vue-cli-service lint"
    ],
    "src/**/*.{vue,htm,html,css,sss,less,scss}": [
      "vue-cli-service lint:style"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "ant-design-vue": "^1.7.5",
    "axios": "^0.21.1",
    "core-js": "^3.12.1",
    "enquire.js": "^2.1.6",
    "good-storage": "^1.1.1",
    "js-cookie": "^2.2.1",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@samhammer/vue-cli-plugin-stylelint": "^2.0.1",
    "@vue/cli-plugin-babel": "~5.0.0-beta.1",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.1",
    "@vue/cli-plugin-pwa": "~5.0.0-beta.1",
    "@vue/cli-plugin-router": "~5.0.0-beta.1",
    "@vue/cli-plugin-vuex": "~5.0.0-beta.1",
    "@vue/cli-service": "~5.0.0-beta.1",
    "@vue/eslint-config-standard": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "deploy-cli-service": "^1.3.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "less": "3.13.1",
    "less-loader": "8.1.1",
    "lint-staged": "^11.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^5.1.1",
    "vue-template-compiler": "^2.6.12"
  }
}
